<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <Pattern>%date{yyyy-MM-dd HH:mm:ss} %-5level [%thread] [%X{traceId}] %class[%F:%L] -- %msg%xEx%n</Pattern>
            <charset>UTF-8</charset>
        </encoder>
    </appender>
    <logger name="com.wygdove" level="DEBUG" />
    <root level="ERROR">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>